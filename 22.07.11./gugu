<script>
document.write("<table border = 1><tr>")
for(i = 1; i <= 10; i++){
  document.write("<td>")
  for(j = 1; j < =9; j++){
    document.write(i, '*', j, '=', i * j, '<br/>')}
  document.write("</td>")}
document.write("</tr><tr>")
for(i = 11; i <= 20; i++){
  document.write("<td>")
  for(j = 1; j < =9; j++){
    document.write(i, '*', j, '=', i * j, '<br/>')}
  document.write("</td>")}
document.write("</tr><table>")
</script>
</body>
